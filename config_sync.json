{
  "wandb": {
    "enabled": true,
    "project": "non-iid实验对比",
    "name": "1"
  },
  "global": {
    "mode": "sync",
    "experiment": "Sync/test/2",
    "stale": {
      "step": 1,
      "shuffle": true,
      "list": [10, 10, 10, 5, 5, 5, 5]
    },
    "data_file": "FashionMNIST",
    "data_name": "FashionMNIST",
    "iid": false,
    "client_num": 50
  },
  "server": {
    "epochs": 2,
    "model_file": "CNN",
    "model_name": "CNN",
    "scheduler": {
      "scheduler_interval": 5,
      "schedule_file": "RandomSchedule",
      "schedule_name": "RandomSchedule",
      "params": {
        "c_ratio": 0.1,
        "schedule_interval": 5
      },
      "receiver": {
        "receiver_file": "AvgReceiver",
        "receiver_name": "AvgReceiver",
        "params": {
        }
      }
    },
    "updater": {
      "update_file": "FedAvg",
      "update_name": "FedAvg",
      "loss": "cross_entropy",
      "params": {
        "a": 10,
        "b": 4,
        "alpha": 0.1,
        "r" : 1,
        "c" : 2,
        "d" : 2
      }
    }
  },
  "client_manager": {
    "checker": {
      "checker_file": "AvgChecker",
      "checker_name": "AvgChecker",
      "params": {
      }
    },
    "client_file": "SyncClient",
    "client_name": "SyncClient"
  },
  "client": {
    "epochs": 2,
    "batch_size": 50,
    "model_file": "CNN",
    "model_name": "CNN",
    "loss": "cross_entropy",
    "mu": 0.01,
    "optimizer": {
      "name": "Adam",
      "lr": 0.01,
      "weight_decay": 0
    }
  }
}