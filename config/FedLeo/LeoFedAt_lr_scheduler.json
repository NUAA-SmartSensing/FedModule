{
    "wandb": {
        "enabled": true,
        "project": "FedLeo",
        "name": "resnet18-LeoFedAt-lr_scheduler"
    },
    "global": {
        "dataset_pre_load": true,
        "use_file_system": false,
        "multi_gpu": false,
        "experiment": "FedLeo/resnet18-LeoFedAt-lr_scheduler",
        "stale": false,
        "dataset": {
            "path": "dataset.CIFAR10.CIFAR10",
            "params": {}
        },
        "iid": false,
        "client_num": 66
    },
    "server": {
        "path": "server.SemiAsyncServer.SemiAsyncServer",
        "epochs": 1500,
        "model": {
            "path": "torchvision.models.resnet.resnet18",
            "params": {
                "num_classes": 10
            }
        },
        "scheduler": {
            "path": "scheduler.LeoSemiAsyncScheduler.LeoSemiAsyncScheduler",
            "schedule": {
                "path": "schedule.FullSchedule.FullSchedule",
                "params": {}
            },
            "scheduler_access": "./src/access/walker_delta_access_info.csv"
        },
        "updater": {
            "path": "updater.SemiAsyncUpdater.SemiAsyncUpdater",
            "update": {
                "path": "update.FedAT.FedAT",
                "params": {}
            },
            "loss": "torch.nn.functional.cross_entropy",
            "params": {},
            "group": {
                "path": "update.FedAvg.FedAvg",
                "params": {}
            }
        }
    },
    "group_manager": {
        "path": "groupmanager.NormalGroupManager.NormalGroupManager",
        "group_method": {
            "path": "group.OrbitGroup.OrbitGroup",
            "params": {
                "group_info": "./src/access/walker_delta_orbit_info.csv"
            }
        }
    },
    "queue_manager": {
        "path": "queuemanager.LeoQueueListManager.LeoQueueListManager",
        "receiver": {
            "path": "receiver.LeoMultiQueueReceiver.LeoMultiQueueReceiver",
            "params": {}
        },
        "checker": {
            "path": "checker.AllChecker.AllChecker",
            "params": {}
        }
    },
    "client_manager": {
        "path": "clientmanager.NormalClientManager.NormalClientManager"
    },
    "client": {
        "path": "client.SemiClient.SemiClient",
        "epochs": 5,
        "batch_size": 50,
        "model": {
            "path": "torchvision.models.resnet.resnet18",
            "params": {
                "num_classes": 10
            }
        },
        "loss": "torch.nn.functional.cross_entropy",
        "mu": 0.001,
        "optimizer": {
            "path": "torch.optim.SGD",
            "params": {
                "lr": 0.1,
                "weight_decay": 0.01
            }
        },
        "lr_scheduler": {
            "path": "torch.optim.lr_scheduler.ExponentialLR",
            "params": {
                "gamma": 0.998
            }
        }
    }
}