{
  "wandb": {
    "enabled": true,
    "project": "LeoSync",
    "name": "FedAvg-baseline-100"
  },
  "global": {
    "dataset_pre_load": true,
    "use_file_system": false,
    "multi_gpu": [
      1,
      1,
      0,
      0
    ],
    "use_step": false,
    "experiment": "LeoSync/FedAvg-baseline-100",
    "stale": false,
    "dataset": {
      "path": "dataset.CIFAR10.CIFAR10",
      "params": {}
    },
    "iid": true,
    "client_num": 100
  },
  "server": {
    "path": "server.SyncServer.SyncServer",
    "epochs": 1000,
    "model": {
      "path": "torchvision.models.resnet.resnet18",
      "params": {
        "num_classes": 10
      }
    },
    "scheduler": {
      "path": "scheduler.SyncScheduler.SyncScheduler",
      "schedule": {
        "path": "schedule.RandomSchedule.RandomSchedule",
        "params": {
          "c_ratio": 0.1
        }
      }
    },
    "updater": {
      "path": "updater.SyncUpdater.SyncUpdater",
      "loss": "torch.nn.functional.cross_entropy",
      "update": {
        "path": "update.FedAvg.FedAvg",
        "params": {}
      }
    }
  },
  "queue_manager": {
    "path": "queuemanager.SingleQueueManager.SingleQueueManager",
    "receiver": {
      "path": "receiver.NormalReceiver.NormalReceiver",
      "params": {}
    },
    "checker": {
      "path": "checker.SyncChecker.SyncChecker",
      "params": {}
    }
  },
  "client_manager": {
    "path": "clientmanager.NormalClientManager.NormalClientManager"
  },
  "client": {
    "path": "client.NormalClient.NormalClient",
    "epochs": 5,
    "batch_size": 50,
    "model": {
      "path": "torchvision.models.resnet.resnet18",
      "params": {
        "num_classes": 10
      }
    },
    "loss": "torch.nn.functional.cross_entropy",
    "mu": 0.001,
    "optimizer": {
      "path": "torch.optim.SGD",
      "params": {
        "lr": 0.1,
        "weight_decay": 0.01
      }
    },
    "lr_scheduler": {
      "path": "torch.optim.lr_scheduler.ExponentialLR",
      "params": {
        "gamma": 0.995
      }
    }
  }
}