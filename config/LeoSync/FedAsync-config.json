{
	"wandb": {
		"enabled": true,
		"project": "FedAsync",
		"name": "1"
	},
	"global": {
		"dataset_pre_load": true,
		"use_file_system": false,
		"multi_gpu": false,
		"use_step": true,
		"experiment": "FedAsync/1",
		"stale": false,
		"dataset": {
			"path": "dataset.MNIST.MNIST",
			"params": {}
		},
		"iid": true,
		"client_num": 100
	},
	"server": {
		"path": "server.AsyncServer.AsyncServer",
		"epochs": 1000,
		"model": {
			"path": "model.LeNet5.LeNet5",
			"params": {}
		},
		"scheduler": {
			"path": "scheduler.AsyncScheduler.AsyncScheduler",
			"schedule_interval": 1,
			"schedule_delay": 1,
			"schedule": {
				"path": "schedule.RandomSchedule.RandomSchedule",
				"params": {
					"c_ratio": 0.4
				}
			}
		},
		"updater": {
			"path": "updater.AsyncUpdater.AsyncUpdater",
			"num_generator": 1,
			"loss": "torch.nn.functional.cross_entropy",
			"update": {
				"path": "update.FedAsync.FedAsync",
				"params": {
					"a": 10,
					"b": 8,
					"alpha": 0.9,
					"r": 1
				}
			}
		}
	},
	"queue_manager": {
		"path": "queuemanager.SingleQueueManager.SingleQueueManager",
		"receiver": {
			"path": "receiver.NormalReceiver.NormalReceiver",
			"params": {}
		},
		"checker": {
			"path": "checker.AllChecker.AllChecker",
			"params": {}
		}
	},
	"client_manager": {
		"path": "clientmanager.NormalClientManager.NormalClientManager"
	},
	"client": {
		"path": "client.NormalClient.NormalClient",
		"epochs": 5,
		"batch_size": 32,
		"test_size": 0.1,
		"test_batch_size": 16,
		"model": {
			"path": "model.LeNet5.LeNet5",
			"params": {}
		},
		"loss": "torch.nn.functional.cross_entropy",
		"mu": 0,
		"optimizer": {
			"path": "torch.optim.SGD",
			"params": {
				"lr": 0.01,
				"weight_decay": 0
			}
		}
	}
}