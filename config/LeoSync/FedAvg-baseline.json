{
	"wandb": {
		"enabled": false,
		"use_step": true,
		"project": "LeoSync",
		"name": "FedAvg-baseline-iid"
	},
	"global": {
		"dataset_pre_load": true,
		"use_file_system": false,
		"multi_gpu": true,
		"use_step": true,
		"experiment": "LeoSync/FedAvg-baseline-iid",
		"stale": false,
		"dataset": {
			"path": "dataset.CIFAR10.CIFAR10",
			"params": {}
		},
		"iid": {
			"group_similarity": true,
			"beta": 1000000
		},
		"client_num": 100
	},
	"server": {
		"path": "server.LeoSyncServer.LeoSyncServer",
		"epochs": 1000,
		"model": {
			"path": "torchvision.models.resnet.resnet18",
			"params": {
				"num_classes": 10
			}
		},
		"scheduler": {
			"path": "scheduler.LeoSyncScheduler.LeoSyncScheduler",
			"schedule_algo": {
				"path": "algorithm.Baseline.FedAvgLeoSync",
				"params": {
					"period": 6000,
					"sample_time": 5
				}
			},
			"schedule": {
				"path": "schedule.FullSchedule.FullSchedule",
				"params": {}
			},
			"scheduler_access": "./src/access/WalkerDelta/walker_delta_access_info.csv"
		},
		"updater": {
			"path": "updater.LeoSyncUpdater.LeoSyncUpdater",
			"loss": "torch.nn.functional.cross_entropy",
			"update": {
				"path": "update.FedAvg.FedAvg",
				"params": {}
			}
		}
	},
	"group_manager": {
		"path": "groupmanager.NormalGroupManager.NormalGroupManager",
		"group_method": {
			"path": "group.OrbitGroup.OrbitGroup",
			"params": {
				"group_info": "./src/access/WalkerDelta/walker_delta_orbit_info.csv"
			}
		}
	},
	"queue_manager": {
		"path": "queuemanager.SingleQueueManager.SingleQueueManager",
		"receiver": {
			"path": "receiver.NormalReceiver.NormalReceiver",
			"params": {}
		},
		"checker": {
			"path": "checker.SyncChecker.SyncChecker",
			"params": {}
		}
	},
	"client_manager": {
		"path": "clientmanager.NormalClientManager.NormalClientManager"
	},
	"client": {
		"path": "client.NormalClient.NormalClient",
		"epochs": 5,
		"batch_size": 50,
		"model": {
			"path": "torchvision.models.resnet.resnet18",
			"params": {
				"num_classes": 10
			}
		},
		"loss": "torch.nn.functional.cross_entropy",
		"mu": 0.001,
		"optimizer": {
			"path": "torch.optim.SGD",
			"params": {
				"lr": 0.1,
				"weight_decay": 0.01
			}
		},
		"lr_scheduler": {
			"path": "torch.optim.lr_scheduler.ExponentialLR",
			"params": {
				"gamma": 0.995
			}
		}
	}
}