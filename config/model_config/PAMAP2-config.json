{
  "wandb": {
    "enabled": false,
    "project": "HAR测试",
    "name": "5"
  },
  "global": {
    "mode": "process",
    "use_file_system": false,
    "multi_gpu": true,
    "experiment": "HAR/123",
    "stale": false,
    "dataset": {
      "path": "dataset.PAMAP2.PAMAP2",
      "params": {}
    },
    "iid":  {
      "beta": 0.5
    },
    "client_num": 25
  },
  "server": {
    "path": "server.SyncServer.SyncServer",
    "epochs": 30,
    "model": {
      "path": "model.HAR.cnn.CNN",
      "params": {
        "train_shape": "self.dataset.train_dataset.data.shape",
        "category": 12
      }
    },
    "scheduler": {
      "path": "scheduler.SyncScheduler.SyncScheduler",
      "schedule": {
        "path": "schedule.RandomSchedule.RandomSchedule",
        "params": {
          "c_ratio": 0.3
        }
      }
    },
    "updater": {
      "path": "updater.SyncUpdater.SyncUpdater",
      "loss": "torch.nn.functional.cross_entropy",
      "update": {
        "path": "update.FedAvg.FedAvg",
        "params": {
        }
      }
    }
  },
  "queue_manager": {
    "path": "queuemanager.SingleQueueManager.SingleQueueManager",
    "receiver": {
      "path": "receiver.NormalReceiver.NormalReceiver",
      "params": {
      }
    },
    "checker": {
      "path": "checker.SyncChecker.SyncChecker",
      "params": {
      }
    }
  },
  "client_manager": {
    "path": "clientmanager.NormalClientManager.NormalClientManager"
  },
  "client": {
    "path": "client.NormalClient.NormalClient",
    "epochs": 1,
    "batch_size": 32,
    "model": {
      "path": "model.HAR.cnn.CNN",
      "params": {
        "train_shape": "train_ds[0][0].shape",
        "category": 12
      }
    },
    "classes": 10,
    "loss": "torch.nn.functional.cross_entropy",
    "mu": 0,
    "optimizer": {
      "path": "torch.optim.SGD",
      "params": {
        "lr": 0.01,
        "weight_decay": 0
      }
    }
  }
}