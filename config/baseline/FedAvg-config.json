{
  "wandb": {
    "enabled": true,
    "project": "FedLeo",
    "name": "FedAvg-baseline"
  },
  "global": {
    "dataset_pre_load": true,
    "use_file_system": false,
    "multi_gpu": false,
    "experiment": "FedLeo/FedAvg-baseline",
    "stale": false,
    "dataset": {
      "path": "dataset.CIFAR10.CIFAR10",
      "params": {}
    },
    "iid": false,
    "client_num": 66
  },
  "server": {
    "path": "server.SyncServer.SyncServer",
    "epochs": 1000,
    "model": {
      "path": "torchvision.models.resnet.resnet18",
      "params": {
        "num_classes": 10
      }
    },
    "scheduler": {
      "path": "scheduler.SyncScheduler.SyncScheduler",
      "schedule": {
        "path": "schedule.FullSchedule.FullSchedule",
        "params": {}
      }
    },
    "updater": {
      "path": "updater.SyncUpdater.SyncUpdater",
      "loss": "torch.nn.functional.cross_entropy",
      "update": {
        "path": "update.FedAvg.FedAvg",
        "params": {}
      }
    }
  },
  "queue_manager": {
    "path": "queuemanager.SingleQueueManager.SingleQueueManager",
    "receiver": {
      "path": "receiver.NormalReceiver.NormalReceiver",
      "params": {}
    },
    "checker": {
      "path": "checker.SyncChecker.SyncChecker",
      "params": {}
    }
  },
  "client_manager": {
    "path": "clientmanager.NormalClientManager.NormalClientManager"
  },
  "client": {
    "path": "client.NormalClient.NormalClient",
    "epochs": 2,
    "batch_size": 64,
    "model": {
      "path": "torchvision.models.resnet.resnet18",
      "params": {
        "num_classes": 10
      }
    },
    "loss": "torch.nn.functional.cross_entropy",
    "mu": 0,
    "optimizer": {
      "path": "torch.optim.SGD",
      "params": {
        "lr": 0.01,
        "weight_decay": 0
      }
    }
  }
}