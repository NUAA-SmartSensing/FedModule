{
  "wandb": {
    "enabled": true,
    "project": "FedAsyncVSFedAT",
    "name": "FedAsync"
  },
  "global": {
    "use_file_system": false,
    "multi_gpu": false,
    "experiment": "FedAsyncVSFedAT/FedAsync2000",
    "stale": {
      "step": 4,
      "shuffle": true,
      "list": [10, 20, 20]
    },
    "dataset": {
      "path": "dataset.FashionMNIST.FashionMNIST",
      "params": {}
    },
    "iid": false,
    "client_num": 50
  },
  "server": {
    "path": "server.AsyncServer.AsyncServer",
    "epochs": 2000,
    "model": {
      "path": "model.LeNet5.LeNet5",
      "params": {}
    },
    "scheduler": {
      "path": "scheduler.AsyncScheduler.AsyncScheduler",
      "schedule_interval": 4,
      "schedule_delay": 50,
      "schedule": {
        "path": "schedule.FullSchedule.FullSchedule",
        "params": {
        }
      }
    },
    "updater": {
      "path": "updater.AsyncUpdater.AsyncUpdater",
      "num_generator": 1,
      "loss": "torch.nn.functional.cross_entropy",
      "update": {
        "path": "update.FedAsync.FedAsync",
        "params": {
          "a": 10,
          "b": 8,
          "alpha": 0.9,
          "r" : 1
        }
      }
    }
  },
  "queue_manager": {
    "path": "queuemanager.SingleQueueManager.SingleQueueManager",
    "receiver": {
      "path": "receiver.NormalReceiver.NormalReceiver",
      "params": {
      }
    },
    "checker": {
      "path": "checker.AllChecker.AllChecker",
      "params": {
      }
    }
  },
  "client_manager": {
    "path": "clientmanager.NormalClientManager.NormalClientManager"
  },
  "client": {
    "path": "client.NormalClient.NormalClient",
    "epochs": 2,
    "batch_size": 32,
    "test_size": 0.1,
    "test_batch_size": 16,
    "model": {
      "path": "model.LeNet5.LeNet5",
      "params": {}
    },
    "loss": "torch.nn.functional.cross_entropy",
    "mu": 0,
    "optimizer": {
      "path": "torch.optim.SGD",
      "params": {
        "lr": 0.01,
        "weight_decay": 0
      }
    }
  }
}