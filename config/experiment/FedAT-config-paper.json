{
  "wandb": {
    "enabled": true,
    "project": "FedAT",
    "name": "paper-setting"
  },
  "global": {
    "use_file_system": false,
    "multi_gpu": true,
    "experiment": "FedAT/paper-setting",
    "stale": {
      "step": 5,
      "shuffle": true,
      "list": [20, 20, 20, 20, 20]
    },
    "dataset": {
      "path": "dataset.FashionMNIST.FashionMNIST",
      "params": {}
    },
    "iid": {
      "customize": true,
      "label": {
        "shuffle":true,
        "step": 1,
        "list": [0,100]
      },
      "data": {
      }
    },
    "client_num": 100
  },
  "server": {
    "path": "server.SemiAsyncServer.SemiAsyncServer",
    "epochs": 50,
    "model": {
      "path": "model.CNN.CNN_CIFAR10",
      "params": {}
    },
    "scheduler": {
      "path": "scheduler.SemiAsyncScheduler.SemiAsyncScheduler",
      "schedule": {
        "path": "schedule.RandomSchedule.RandomSchedule",
        "params": {
          "c_ratio": 0.5
        }
      }
    },
    "updater": {
      "path": "updater.SemiAsyncUpdater.SemiAsyncUpdater",
      "update": {
        "path": "update.FedAT.FedAT",
        "params": {
        }
      },
      "loss": "torch.nn.functional.cross_entropy",
      "params": {
      },
      "group": {
        "path": "update.FedAvg.FedAvg",
        "params": {
        }
      }
    }
  },
  "group_manager": {
    "path": "groupmanager.NormalGroupManager.NormalGroupManager",
    "group_method": {
      "path": "group.DelayGroup.DelayGroup",
      "params": {
        "step": 5
      }
    }
  },
  "queue_manager": {
    "path": "queuemanager.QueueListManager.QueueListManager",
    "receiver": {
      "path": "receiver.MultiQueueReceiver.MultiQueueReceiver",
      "params": {
      }
    },
    "checker": {
      "path": "checker.AllChecker.AllChecker",
      "params": {
      }
    }
  },
  "client_manager": {
    "path": "clientmanager.NormalClientManager.NormalClientManager"
  },
  "client": {
    "path": "client.SemiClient.SemiClient",
    "epochs": 3,
    "batch_size": 10,
    "model": {
      "path": "model.CNN.CNN",
      "params": {}
    },
    "loss": "torch.nn.functional.cross_entropy",
    "mu": 0.4,
    "optimizer": {
      "path": "torch.optim.Adam",
      "params": {
        "lr": 0.01,
        "weight_decay": 0
      }
    }
  }
}