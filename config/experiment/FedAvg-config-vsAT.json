{
  "wandb": {
    "enabled": true,
    "project": "FedAT",
    "name": "baseline-iid-FedAvg"
  },
  "global": {
    "use_file_system": false,
    "multi_gpu": true,
    "experiment": "FedAT/baseline-iid-FedAvg",
    "stale": {
      "step": 5,
      "shuffle": false,
      "list": [0, 30, 20]
    },
    "dataset": {
      "path": "dataset.FashionMNIST.FashionMNIST",
      "params": {}
    },
    "iid": true,
    "client_num": 50
  },
  "server": {
    "path": "server.SyncServer.SyncServer",
    "epochs": 50,
    "model": {
      "path": "model.CNN.CNN",
      "params": {}
    },
    "scheduler": {
      "path": "scheduler.SyncScheduler.SyncScheduler",
      "schedule": {
        "path": "schedule.RandomSchedule.RandomSchedule",
        "params": {
          "c_ratio": 0.3
        }
      }
    },
    "updater": {
      "path": "updater.SyncUpdater.SyncUpdater",
      "loss": "torch.nn.functional.cross_entropy",
      "update": {
        "path": "update.FedAvg.FedAvg",
        "params": {
        }
      }
    }
  },
  "queue_manager": {
    "path": "queuemanager.SingleQueueManager.SingleQueueManager",
    "receiver": {
      "path": "receiver.NormalReceiver.NormalReceiver",
      "params": {
      }
    },
    "checker": {
      "path": "checker.SyncChecker.SyncChecker",
      "params": {
      }
    }
  },
  "client_manager": {
    "path": "clientmanager.NormalClientManager.NormalClientManager"
  },
  "client": {
    "path": "client.NormalClient.NormalClient",
    "epochs": 2,
    "batch_size": 50,
    "model": {
      "path": "model.CNN.CNN",
      "params": {}
    },
    "loss": "torch.nn.functional.cross_entropy",
    "mu": 0,
    "optimizer": {
      "path": "torch.optim.SGD",
      "params": {
        "lr": 0.01,
        "weight_decay": 0
      }
    }
  }
}