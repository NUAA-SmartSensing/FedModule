{
  "wandb": {
    "enabled": true,
    "project": "FedAT",
    "name": "baseline-iid-FedAsync"
  },
  "global": {
    "use_file_system": false,
    "multi_gpu": true,
    "experiment": "FedAsync/baseline-iid-FedAsync",
    "stale": {
      "step": 5,
      "shuffle": false,
      "list": [0, 30, 20]
    },
    "dataset": {
      "path": "dataset.FashionMNIST.FashionMNIST",
      "params": {}
    },
    "iid": true,
    "client_num": 50
  },
  "server": {
    "path": "server.AsyncServer.AsyncServer",
    "epochs": 300,
    "model": {
      "path": "model.CNN.CNN",
      "params": {}
    },
    "scheduler": {
      "path": "scheduler.AsyncScheduler.AsyncScheduler",
      "schedule_interval": 5,
      "schedule_delay": 10,
      "schedule": {
        "path": "schedule.RandomSchedule.RandomSchedule",
        "params": {
          "c_ratio": 0.4
        }
      }
    },
    "updater": {
      "path": "updater.AsyncUpdater.AsyncUpdater",
      "num_generator": 1,
      "loss": "torch.nn.functional.cross_entropy",
      "update": {
        "path": "update.FedAsync.FedAsync",
        "params": {
          "a": 10,
          "b": 8,
          "alpha": 0.9,
          "r" : 1
        }
      }
    }
  },
  "queue_manager": {
    "path": "queuemanager.SingleQueueManager.SingleQueueManager",
    "receiver": {
      "path": "receiver.NormalReceiver.NormalReceiver",
      "params": {
      }
    },
    "checker": {
      "path": "checker.AllChecker.AllChecker",
      "params": {
      }
    }
  },
  "client_manager": {
    "path": "clientmanager.NormalClientManager.NormalClientManager"
  },
  "client": {
    "path": "client.NormalClient.NormalClient",
    "epochs": 2,
    "batch_size": 50,
    "test_size": 0.1,
    "test_batch_size": 16,
    "model": {
      "path": "model.CNN.CNN",
      "params": {}
    },
    "loss": "torch.nn.functional.cross_entropy",
    "mu": 0,
    "optimizer": {
      "path": "torch.optim.SGD",
      "params": {
        "lr": 0.01,
        "weight_decay": 0
      }
    }
  }
}