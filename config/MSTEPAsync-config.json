{
  "wandb": {
    "enabled": false,
    "project": "MStep",
    "name": "1"
  },
  "global": {
    "multi_gpu": true,
    "mode": "async",
    "experiment": "MStep/1",
    "stale": {
      "step": 2,
      "shuffle": true,
      "list": [2, 2, 2, 2, 2]
    },
    "dataset_path": "dataset.FashionMNIST.FashionMNIST",
    "iid":  {
      "beta": 0.5
    },
    "client_num": 10
  },
  "server": {
    "epochs": 20,
    "model": {
      "path": "model.CNN.CNN",
      "params": {}
    },
    "scheduler": {
      "scheduler_path": "schedule.NoSchedule.NoSchedule",
      "params": {
        "schedule_interval": 1
      },
      "receiver": {
        "receiver_path": "fedasync.receiver.AvgReceiver.AvgReceiver",
        "params": {
        }
      }
    },
    "updater": {
      "updater_path": "update.StepAsyncAvg.StepAsyncAvg",
      "loss": "cross_entropy",
      "nums": 3,
      "params": {
        "rho": 0.2,
        "alpha": 0.5
      }
    }
  },
  "client_manager": {
    "client_path": "client.ActiveClient.ActiveClient",
    "checker": {
      "checker_path": "fedasync.checker.AllChecker.AllChecker",
      "params": {
      }
    }
  },
  "client": {
    "epochs": 1,
    "batch_size": 50,
    "model": {
      "path": "model.CNN.CNN",
      "params": {}
    },
    "acquire_model_delay": 1,
    "loss": "cross_entropy",
    "mu": 0.01,
    "optimizer": {
      "path": "torch.optim.SGD",
      "weight_decay": 0,
      "lr": 0.01
    }
  }
}